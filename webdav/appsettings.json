{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WebDav": {
    "Address": "0.0.0.0",
    "Port": 6065,
    "Tls": false,
    "Cert": null,
    "Key": null,
    "Prefix": "/dav",
    "Debug": false,
    "NoSniff": false,
    "NoPassword": false,
    "BehindProxy": false,
    "Directory": "./data",
    "Permissions": "R",
    "RulesBehavior": "overwrite",
    "Log": {
      "Format": "console",
      "Colors": true,
      "Outputs": ["stderr"]
    },
    "Cors": {
      "Enabled": false,
      "Credentials": false,
      "AllowedHeaders": ["*"],
      "AllowedHosts": ["*"],
      "AllowedMethods": ["*"],
      "ExposedHeaders": []
    },
    "Users": [],
    "Rules": []
  }
}
